:javascript
  $(document).ready(function() {
      update = function(){
          $('#clock').html("Hello");
      });
      update();
      setInterval(update, 1000);
  });
%h1 Add New Timestamp
.row-fluid
  #clock
    = simple_form_for @timestamp do |f| 
      .control-group
        .controls
          =f.association :project
      .control-group
        .controls
          =f.association :client
      .control-group
        .controls
          =f.submit "Add new Timestmp", id: "add_timestamp", class: "btn btn-primary"

            


